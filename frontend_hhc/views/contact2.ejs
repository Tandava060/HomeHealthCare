<header>
    <!--? Header Start -->
    <%- include('partials/navbar.ejs') %>
    <!-- Header End -->
</header>
    <main>
        <% if (status) {%> 
            <div class="container">
            <% if (status == true) {%>  
                <h3 style="text-align: center;font-size: 28px;margin: 5% auto;font-weight: bold;">Your message has been sent succesfully. We will revert back you to you soon.</h3>
                <% } else { %>
                    <div style="text-align: center;font-size: 25px;margin: 45% auto;font-weight: bold;">
                        <h3>Your message was not sent succesfully.</h3>
                        <h4>Try again later.</h4>
                    </div>
                  
                <% } %>
            </div>
            <% } else { %>
        <div class="contact-form-main">
            <div class="container">
              
                <div class="row justify-content-end">
                    <div class="col-xl-7 col-lg-7">
                        <div class="form-wrapper">
                            <!--Section Tittle  -->
                            <div class="form-tittle">
                                <div class="row ">
                                    <div class="col-xl-12">
                                        <div class="section-tittle section-tittle2">
                                            <span>Appointment Apply Form</span>
                                            <h2>Appointment Form</h2>
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <!--End Section Tittle  -->
                            <form id="contact-form" action="/contactAbout" method="POST">
                                <div class="row">
                                    <div class="col-lg-6 col-md-6">
                                        <div class="form-box user-icon mb-30">
                                            <input type="text" name="name" placeholder="Name">
                                        </div>
                                    </div>
                                    <div class="col-lg-6 col-md-6">
                                        <div class="form-box email-icon mb-30">
                                            <input type="text" name="phone" placeholder="Phone">
                                        </div>
                                    </div>
                                    <div class="col-lg-6 col-md-6 mb-30">
                                        <div class="select-itms">
                                            <select name="service" id="select2">
                                                <option value="health">Health Law</option>
                                                <option value="test">testing</option>
                                            </select>
                                        </div>
                                    </div>
                                    <div class="col-lg-6 col-md-6">
                                        <div class="form-box subject-icon mb-30">
                                            <input type="Email" name="email" placeholder="Email">
                                        </div>
                                    </div>
                                    <div class="col-lg-12">
                                        <div class="form-box message-icon mb-65">
                                            <textarea name="description" id="message" placeholder="Message"></textarea>
                                        </div>
                                        <div class="submit-info">
                                            <button class="btn" type="submit">Submit Now <i class="ti-arrow-right"></i> </button>
                                        </div>
                                    </div>
                                </div>
                            </form>
                        </div>
                    </div>
                </div>
            
            </div>
        <!-- gallery Products End -->
        <div class="from-left d-none d-lg-block">
            <img src="assets/img/gallery/contact_form.png" alt="">
        </div>
        </div>
        <% } %>
    </main>

    <footer>
        <!--? Footer Start-->
        <%- include('partials/footer.ejs') %>
        <!-- Footer End-->
    </footer>
    <!-- Scroll Up -->
    <div id="back-top" >
        <a title="Go to Top" href="#"> <i class="fas fa-level-up-alt"></i></a>
    </div>

    <!-- JS here -->

    <script src="./assets/js/vendor/modernizr-3.5.0.min.js"></script>
    <!-- Jquery, Popper, Bootstrap -->
    <script src="./assets/js/vendor/jquery-1.12.4.min.js"></script>
